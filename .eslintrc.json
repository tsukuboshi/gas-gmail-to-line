{
  "env": {
    "browser": true,
    "node": true,
    "es2020": true
  },
  "extends": ["./node_modules/gts", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "@typescript-eslint/no-floating-promises": "off",
    "@typescript-eslint/no-unused-vars": ["error", {
      "varsIgnorePattern": "^(main|testEvents|onOpen|initializeSpreadsheet|filterRecentlyUpdatedEvents|setupOrUpdateTrigger|testGmailToLineIntegration)$"
    }]
  },
  "ignorePatterns": ["template/**/*"]
}
